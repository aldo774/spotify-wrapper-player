{
  "name": "spotify-wrapper-player",
  "version": "1.0.0",
  "description": "A player using the spotify-wrapper lib.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm test && npm run test:code-coverage",
    "test": "./node_modules/.bin/mocha src/tests/**/*.spec.js",
    "test:code-coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldo774/js_tdd_course.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Aldo Soares <soares_aldo@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldo774/js_tdd_course/issues"
  },
  "homepage": "https://github.com/aldo774/js_tdd_course#readme",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0"
  }
}
